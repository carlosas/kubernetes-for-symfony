{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "symfony"
    },
    "spec": {
        "selector": {
            "app": "symfony"
        },
        "type": "NodePort",
        "ports": [
            {
	        "name": "http",
                "protocol": "TCP",
                "port": 80,
                "targetPort": 80,
                "nodePort": 30080
            },
            {
	        "name": "https",
                "protocol": "TCP",
                "port": 443,
                "targetPort": 443,
                "nodePort": 30443
            }
        ]
    }
}
